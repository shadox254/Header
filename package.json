{
  "name": "custom-42header",
  "displayName": "Custom 42 Header",
  "description": "A custom header.",
  "version": "1.0.0",
  "publisher": "shadox",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shadox254/Header"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "42header.insertHeader",
        "title": "Insert Custom 42 Header"
      }
    ],
    "keybindings": [
      {
        "command": "42header.insertHeader",
        "key": "ctrl+alt+h",
        "mac": "cmd+alt+h",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Custom 42 Header Settings",
      "properties": {
        "42header.username": {
          "type": "string",
          "default": "",
          "description": "rruiz."
        },
        "42header.email": {
          "type": "string",
          "default": "",
          "description": "rruiz@student.42lehavre.fr."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.x"
  }
}
